{
  "name": "exewen/robot-chat",
  "description": "A robot-chat component for Exewen.",
  "authors": [
    {
      "name": "exewen",
      "email": "exeweb@163.com"
    }
  ],
  "license": "MIT",
  "type": "library",
  "keywords": [
    "php",
    "exewen",
    "robot",
    "chat",
    "qyweixin chat",
    "dingding chat"
  ],
  "require": {
    "php": ">=7.3",
    "exewen/facades": "^1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 | ^6.5 | ^7.5 | ^8.4 | ^9.3"
  },
  "autoload": {
    "psr-4": {
      "Exewen\\RobotChat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ExewenTest\\RobotChat\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "extra": {
    "exewen": {
      "config": "Exewen\\RobotChat\\ConfigRegister"
    }
  }
}
